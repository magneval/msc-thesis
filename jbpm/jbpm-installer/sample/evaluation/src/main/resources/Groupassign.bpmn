        <di:waypoint x="124" y="124" />
        <di:waypoint x="252" y="125" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="_2-_3" >
        <di:waypoint x="252" y="125" />
